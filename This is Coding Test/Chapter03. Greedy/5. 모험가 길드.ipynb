   "source": [
    "n = int(input())\n",
    "data = list(map(int, input().split()))\n",
    "data.sort()\n",
    "\n",
    "count = 0 # 현재 index 비교용.\n",
    "answer = 0 # 그룹의 수.\n",
    "\n",
    "for i in data:\n",
    "    count += 1\n",
    "    if count == i:\n",
    "        answer += 1\n",
    "        count = 0\n",
    "        \n",
    "print(answer)"
   ]
